{
  "source": {
    "include": [
      "src/core",
      "src/class",
      "./README.md"
    ]
  },
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": true,
    "default": {
      "outputSourceFiles": true,
      "includeDate": false,
      "useLongnameInNav": false,
      "systemName": "Common Modules",
      "footer": "",
      "navType": "vertical",
      "layoutFile": "plugins/layout.tmpl"
    }
  },
  "plugins": ["plugins/delFooter.js"],
  "docdash": {
    "search": true,
    "collapse": true,
    "footer": "",
    "meta": {
      "title": "函数工具库",
      "description": "日常 JavaScript  函数工具库，方便查看",
      "keyword": "javaScript, utils, jsdoc"
    },
    "menu": {
      "GitHub": {
        "href": "https://github.com/one-pupil/js-utils",
        "class": "menu-item",
        "id": "website_link"
      }
    }
  }
}